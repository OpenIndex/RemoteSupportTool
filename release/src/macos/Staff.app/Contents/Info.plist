<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>CFBundleInfoDictionaryVersion</key>
        <string>6.0</string>

        <key>CFBundleIdentifier</key>
        <string>de.openindex.support.staff.default</string>

        <key>CFBundleName</key>
        <string>StaffSupport</string>

        <key>CFBundleDisplayName</key>
        <string>StaffSupportTool</string>

        <key>CFBundleVersion</key>
        <string>{VERSION}</string>

        <key>CFBundlePackageType</key>
        <string>APPL</string>

        <key>CFBundleExecutable</key>
        <string>JavaMacLauncher</string>

        <key>CFBundleIconFile</key>
        <string>share/icon.icns</string>

        <key>CFBundleDevelopmentRegion</key>
        <string>en-US</string>

        <key>LSApplicationCategoryType</key>
        <string>public.app-category.utilities</string>

        <key>LSMinimumSystemVersion</key>
        <string>10.9</string>

        <key>NSHumanReadableCopyright</key>
        <string>Â©2015-2021 OpenIndex.de</string>

        <key>NSHighResolutionCapable</key>
        <true/>

        <key>JavaMacLauncher</key>
        <dict>
            <key>JavaHome</key>
            <string>Contents/Resources</string>
            <key>JavaCommand</key>
            <string>Contents/Resources/bin/java</string>
            <key>JavaOptions</key>
            <array>
                <string>-Dfile.encoding=UTF-8</string>
            </array>
            <key>JavaModulePath</key>
            <array>
                <string>Contents/Resources/modules</string>
            </array>
            <key>ApplicationCommand</key>
            <string>-m de.openindex.support.staff/de.openindex.support.staff.StaffApplication</string>
            <key>HeapMinimum</key>
            <string>32m</string>
            <key>HeapMaximum</key>
            <string>512m</string>
            <key>DockName</key>
            <dict>
                <key>default</key>
                <string>Staff Support</string>
                <key>de</key>
                <string>Mitarbeiter Support</string>
            </dict>
            <key>DockIcon</key>
            <string>Contents/Resources/share/icon.icns</string>
            <key>UseScreenMenuBar</key>
            <true/>
            <key>LaunchInForeground</key>
            <true/>
        </dict>
    </dict>
